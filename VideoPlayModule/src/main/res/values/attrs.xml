<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="exoVideoAnim">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleSpacing" format="dimension" />
        <attr name="cycle" format="integer" />
    </declare-styleable>

    <declare-styleable name="VideoPlayerView">
        <attr name="use_artwork" format="boolean"/>
        <attr name="default_artwork" format="reference"/>
        <attr name="use_controller" format="boolean"/>
        <attr name="hide_on_touch" format="boolean"/>
        <attr name="resize_mode"/>
        <attr name="show_timeout"/>
        <attr name="rewind_increment"/>
        <attr name="fastforward_increment"/>
    </declare-styleable>
</resources>
